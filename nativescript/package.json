    "nativescript-dev-sass": "^1.6.0",
    "nativescript-dev-typescript": "~0.7.4",
    "nativescript-dev-webpack": "^0.16.2",
    "nativescript-worker-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
